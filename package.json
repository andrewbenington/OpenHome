{
  "productName": "OpenHome",
  "name": "openhome",
  "version": "0.5.0",
  "description": "A tool for moving Pok√©mon between game saves",
  "keywords": [
    "pokemon",
    "home",
    "openhome"
  ],
  "homepage": "https://github.com/andrewbenington/OpenHome#readme",
  "bugs": {
    "url": "https://github.com/andrewbenington/OpenHome/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewbenington/OpenHome.git"
  },
  "license": "GPL-3.0-only",
  "author": {
    "name": "Andrew Benington",
    "email": "andrewbenington57@gmail.com"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "./buildVersionJSON.sh && electron-vite preview",
    "dev:unix": "./buildVersionJSON.sh && electron-vite dev",
    "dev:win": "powershell -ExecutionPolicy Bypass -File ./buildVersionJSON.ps1 && electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "build:no-mac": "electron-vite build && electron-builder --linux  --win --config",
    "build:all": "electron-vite build && electron-builder --linux --mac --win --config",
    "release": "electron-vite build && electron-builder -wl",
    "release-mac": "electron-vite build && electron-builder --mac -p always",
    "compare": "ts-node src/types/pkm_old/__test__/all.test.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/joy": "^5.0.0-beta.48",
    "big-integer": "^1.6.51",
    "buffer": "^6.0.3",
    "chart.js": "^4.2.0",
    "dayjs": "^1.11.12",
    "electron-updater": "^6.3.0",
    "esbuild": "^0.24.0",
    "fp-ts": "^2.16.9",
    "hex-editor-react": "^0.1.0",
    "hexy": "^0.3.5",
    "lodash": "^4.17.21",
    "pokemon-files": "^0.3.5",
    "pokemon-resources": "^1.0.10",
    "pokemon-species-data": "^0.4.2",
    "prando": "^6.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-data-grid": "^7.0.0-beta.44",
    "react-error-boundary": "^4.0.12",
    "react-icons": "^5.3.0",
    "react-json-tree": "^0.18.0",
    "use-file-picker": "^1.5.2",
    "vite-plugin-node-polyfills": "^0.22.0"
  },
  "resolutions": {
    "@mui/material": "npm:@mui/joy@latest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.199",
    "@types/node": "^22.9.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-jest": "^29.7.0",
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3",
    "electron-vite": "^1.0.27",
    "eslint": "^9.14.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "jest": "^29.7.0",
    "prettier": "^3.0.2",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^4.5.5"
  }
}
