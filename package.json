{
  "productName": "OpenHome",
  "name": "openhome",
  "version": "1.9.2",
  "description": "A tool for moving Pok√©mon between game saves",
  "keywords": [
    "pokemon",
    "home",
    "openhome"
  ],
  "homepage": "https://github.com/andrewbenington/OpenHome#readme",
  "bugs": {
    "url": "https://github.com/andrewbenington/OpenHome/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewbenington/OpenHome.git"
  },
  "packageManager": "pnpm@10.30.2",
  "license": "GPL-3.0-only",
  "author": {
    "name": "Andrew Benington",
    "email": "andrewbenington57@gmail.com"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "lint": "pnpm run lint:ci --fix",
    "lint:ci": "eslint src --max-warnings 0 --report-unused-disable-directives -c eslint.config.mjs",
    "typecheck": "tsgo",
    "build": "pnpm run wasm-compile && pnpm vite build",
    "dev": "pnpm run wasm-compile && pnpm vite",
    "tauri": "tauri",
    "test-cargo": "cd pkm_rs_types && cargo test; cd ../pkm_rs_resources && cargo test; cd ../pkm_rs && cargo test; cd ../src-tauri && cargo test",
    "test-vite": "vitest run",
    "test": "pnpm run test-cargo && pnpm run test-vite",
    "wasm-compile": "wasm-pack build ./pkm_rs --target web -- --features wasm",
    "precommit": "pnpm run typecheck && pnpm run format && pnpm run lint",
    "postinstall": "pnpm run wasm-compile"
  },
  "dependencies": {
    "@dnd-kit/collision": "^0.3.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/react": "^0.3.2",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/themes": "^3.3.0",
    "@tanstack/react-virtual": "^3.13.19",
    "@tauri-apps/api": "2.10.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-os": "^2.3.2",
    "chart.js": "^4.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "fp-ts": "^2.16.11",
    "prando": "^6.0.1",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-chartjs-2": "^5.3.1",
    "react-data-grid": "7.0.0-beta.59",
    "react-error-boundary": "^6.1.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.13.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.9.0",
    "@tauri-apps/cli": "^2.10.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.24",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@typescript/native-preview": "7.0.0-dev.20260228.1",
    "@vitejs/plugin-react": "^5.1.4",
    "better-sqlite3": "^12.6.2",
    "camelcase-keys": "^10.0.2",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "madge": "^8.0.0",
    "prettier": "^3.8.1",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
