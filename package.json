{
  "productName": "OpenHome",
  "name": "openhome",
  "version": "1.4.11",
  "description": "A tool for moving Pok√©mon between game saves",
  "keywords": [
    "pokemon",
    "home",
    "openhome"
  ],
  "homepage": "https://github.com/andrewbenington/OpenHome#readme",
  "bugs": {
    "url": "https://github.com/andrewbenington/OpenHome/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewbenington/OpenHome.git"
  },
  "license": "GPL-3.0-only",
  "author": {
    "name": "Andrew Benington",
    "email": "andrewbenington57@gmail.com"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint src --report-unused-disable-directives -c eslint.config.mjs --fix",
    "lint:ci": "eslint src --report-unused-disable-directives -c eslint.config.mjs",
    "typecheck": "tsc",
    "dev:unix": "./buildVersionJSON.sh && tauri dev",
    "dev:win": "powershell -ExecutionPolicy Bypass -File ./buildVersionJSON.ps1 && tauri dev",
    "wasm": "wasm-pack build ./pokemon-wasm --target web",
    "build": "tsc && vite build",
    "dev": "npm run wasm && vite",
    "tauri": "tauri",
    "test": "jest",
    "precommit": "prettier --write . && eslint src --report-unused-disable-directives -c eslint.config.mjs --fix"
  },
  "dependencies": {
    "@dnd-kit/abstract": "0.1.19",
    "@dnd-kit/collision": "0.1.19",
    "@dnd-kit/dom": "0.1.19",
    "@dnd-kit/geometry": "0.1.19",
    "@dnd-kit/helpers": "0.1.19",
    "@dnd-kit/react": "0.1.19",
    "@dnd-kit/state": "0.1.19",
    "@radix-ui/themes": "^3.2.1",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/plugin-dialog": "^2.3.1",
    "@tauri-apps/plugin-fs": "^2.4.1",
    "@tauri-apps/plugin-os": "^2.3.0",
    "@tauri-apps/plugin-shell": "^2.3.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.5.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "fp-ts": "^2.16.10",
    "lodash": "^4.17.21",
    "pokemon-resources": "^1.0.12",
    "pokemon-species-data": "^0.4.7",
    "prando": "^6.0.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-data-grid": "^7.0.0-beta.56",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.2.0",
    "@tauri-apps/cli": "^2.7.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "react-dom": "^19.1.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-wasm-pack": "^0.1.12"
  }
}
