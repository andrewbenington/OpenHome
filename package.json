    "name": "Andrew Benington",
    "email": "andrewbenington57@gmail.com"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint src --max-warnings 0 --report-unused-disable-directives -c eslint.config.mjs --fix",
    "lint:ci": "eslint src --max-warnings 0 --report-unused-disable-directives -c eslint.config.mjs",
    "typecheck": "tsc",
    "dev:unix": "./buildVersionJSON.sh && tauri dev",
    "dev:win": "powershell -ExecutionPolicy Bypass -File ./buildVersionJSON.ps1 && tauri dev",
    "build": "npm run wasm && vite build",
    "dev": "npm run wasm && vite",
    "tauri": "tauri",
    "test": "vitest run",
    "wasm": "wasm-pack build ./pkm_rs_resources --target web -- --features wasm",
    "precommit": "prettier --write . && eslint src --report-unused-disable-directives -c eslint.config.mjs --fix"
  },
  "dependencies": {
    "@dnd-kit/collision": "^0.1.21",
    "@dnd-kit/react": "^0.1.21",
    "@dnd-kit/sortable": "^10.0.0",
    "@radix-ui/themes": "^3.2.1",
    "@tanstack/react-virtual": "^3.13.12",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-os": "^2.3.2",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "chart.js": "^4.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "fp-ts": "^2.16.11",
    "lodash": "^4.17.21",
    "prando": "^6.0.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.1",
    "react-data-grid": "^7.0.0-beta.58",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tauri-apps/cli": "^2.9.4",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.6.2",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  }
